group cation type 1 2 3 4
group anion type 5 6 7 8 9
group sol type 1 2 3 4 5 6 7 8 9
group ele type 10 11 12 13 14 15

variable nsteps    equal 1000000
variable nprint    equal 100
variable nenrg     equal 100
variable ndump     equal 100
variable nwrite    equal 100000

neighbor            2.0 bin
timestep            2. 

thermo_style custom etotal ke temp pe ebond eangle edihed eimp evdwl ecoul elong press density vol v_nsteps cpuremain
thermo_modify flush yes line multi
thermo ${nprint}

run_style           verlet

velocity all create 300.0 32156

fix 0 ele setforce 0.0 0.0 0.0
fix 1 sol nvt temp 400.0 400.0 100.0
#fix e all conp 1 1.015 601 602 603 -0.0 0.0 inv electrode.txt

minimize 1.0e-4 1.0e-6 1000 1000

dump dCONF all custom ${ndump} dump-new.xyz id element  x y z q
dump_modify dCONF element P C C O C S N O F Cc Ce Co Oe Oh H sort id

restart ${nwrite} restart.lmp
run ${nsteps}
write_restart restart.*.lmp
write_data data.*.lmp

